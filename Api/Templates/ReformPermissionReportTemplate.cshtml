@using System;
@using System.Globalization;
@using System.Threading;
@using Api.HelperClasses;
@using Application.Abstractions
@using System.Threading.Tasks
@inject IReportTemplateService reportTemplateService
@model Saaed360Modern.Contracts.Wcf.ReportDTO
@using System.Linq

@{
    Layout = null;
}
@{
    var flow = "ltr";
    var language = Model.Language;
    var align = "left";
    if (language == "ar")
    {
        Thread.CurrentThread.CurrentCulture = CultureInfo.CreateSpecificCulture("ar");
        Thread.CurrentThread.CurrentUICulture = CultureInfo.CreateSpecificCulture("ar");
        Thread.CurrentThread.CurrentCulture.NumberFormat = Thread.CurrentThread.CurrentUICulture.NumberFormat = new CultureInfo("en-US").NumberFormat;
        Thread.CurrentThread.CurrentCulture.DateTimeFormat = Thread.CurrentThread.CurrentUICulture.DateTimeFormat = new CultureInfo("en-US").DateTimeFormat;

        flow = "rtl";
        align = "right";
    }
    else
    {
        Thread.CurrentThread.CurrentCulture = CultureInfo.CreateSpecificCulture("en");
        Thread.CurrentThread.CurrentUICulture = CultureInfo.CreateSpecificCulture("en");
        Thread.CurrentThread.CurrentCulture.NumberFormat = Thread.CurrentThread.CurrentUICulture.NumberFormat = new CultureInfo("en-US").NumberFormat;
        Thread.CurrentThread.CurrentCulture.DateTimeFormat = Thread.CurrentThread.CurrentUICulture.DateTimeFormat = new CultureInfo("en-US").DateTimeFormat;

        flow = "ltr";
        align = "left";
    }
}

<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <style>
        td label {
        font-weight: bold;
        }

        h3 {
        font-weight: bold;
        }

        .page_head .police_logo {
        position: absolute;
        left: 15px;
        top: 8px;
        }

        .page_head .accident_logo {
        position: absolute;
        right: 170px;
        top: 5px;
        }

        .page_head .saaed_logo {
        position: absolute;
        right: 15px;
        top: 12px;
        }

        body {
        font-family: Arial, Helvetica, sans-serif;
        font-size: 15px;
        }
    </style>

</head>
<body dir="@flow" lang="@language">
    <div class="book">
        @* Basic Incident Details Page *@

        <div class="page">
            <div class="subpage">
                <!-- Page 1/2 -->
                <div class="page_head">
                    <img src="data:image/png;base64," width="140" class="saaed_logo" />
                    <img src="data:image/png;base64," width="58" class="police_logo" />
                    @*<img src="data:image/png;base64," width="247" class="accident_logo" />*@

                    <center style="padding-right: 200px;">
                        <h1 style="font-weight:bold">شركة ساعد للأنظمة المرورية </h1>
                        <label style="font-weight:bold">إذن إصلاح وسمكرة لرقم التقرير  &nbsp;	&nbsp;	  @(Model.BasicIncidentDetails.ReportNumber ?? "")</label>
                    </center>
                </div>
                <br /><br />
                <div>

                </div>
                <hr style="background-color:#000000;color:#000000;" />
                <hr style="height:3px;background-color:#000000;color:#000000;" />
                <table width="100%">
                    <tr>
                        <td> <label style="font-weight:bold">ساري المفعول لمدة 30 يوما من تاريخ الحادث، ويخضع لتطبيق مواد قانون السير والمرور في حالة عدم الإصلاح خلال المدة المقررة</label></td>
                    </tr>
                </table>

                <!--Page 3/3 -->
                <table width="100%">
                    <tr>
                        <td colspan="6" style="border-bottom:1px solid #000000;">
                            <h1 style="background-color:#cccccc; color:#000000;height:30px; width:150px;margin:0; line-height:30px; padding:15px; border-size:border-box;">  @MdtBl.MdtBlResource.AccidentBasicDetails</h1>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label style="font-weight:bold">تاريخ الحادث</label>
                        </td>
                        <td>
                            <span>@(Model.BasicIncidentDetails.ReportDateTime != null ? Model.BasicIncidentDetails.ReportDateTime.Date.ToString("dd/MM/yyyy") : "")</span>
                        </td>
                        <td>
                            <label style="font-weight:bold">وقت الحادث</label>
                        </td>
                        <td>
                            <span>
                                @(Model.BasicIncidentDetails.ReportDateTime != null ? Model.BasicIncidentDetails.ReportDateTime.TimeOfDay.ToString(@"hh\:mm\:ss") : "")
                            </span>
                        </td>
                        <td>


                            <label style="font-weight:bold"> @MdtBl.MdtBlResource.ReportType </label>

                        </td>
                        <td>
                            <span>@(Model.BasicIncidentDetails.ReportType ?? "") </span>


                        </td>
                    </tr>
                    <tr class="alt">
                        <td>
                            <label style="font-weight:bold"> @MdtBl.MdtBlResource.Emirate </label>
                        </td>
                        <td>
                            <span>@(Model.BasicIncidentDetails.Emirate ?? "") </span>


                        </td>
                        <td>
                            <label style="font-weight:bold">@MdtBl.MdtBlResource.City </label>
                        </td>
                        <td>
                            <span>@(Model.BasicIncidentDetails.City ?? "") </span>
                        </td>
                        <td>


                            <label style="font-weight:bold"> @MdtBl.MdtBlResource.Region </label>

                        </td>
                        <td>
                            <span>@(Model.BasicIncidentDetails.Region ?? "") </span>


                        </td>
                    </tr>
                    <tr>
                        <td>

                            <label style="font-weight:bold"> @MdtBl.MdtBlResource.AccidentType </label>
                        </td>

                        @if (Model.BasicIncidentDetails.AccidentType == "أخرى(حدد)")
                        {
                            <td colspan="5">
                                <span>
                                    @Model.BasicIncidentDetails.AccidentTypeComment
                                </span>
                            </td>  }
                        else
                        {
                            <td colspan="5">
                                <span>
                                    @Model.BasicIncidentDetails.AccidentType
                                </span>
                            </td> }


                    </tr>
                    <tr class="alt">

                        <td>


                            <label style="font-weight:bold"> @MdtBl.MdtBlResource.Description </label>

                        </td>
                        <td colspan="5">
                            <span class="multi">
                                @(Model.BasicIncidentDetails.Description ?? "")

                            </span>
                        </td>

                    </tr>
                    <tr>
                        <td>

                            <label> @MdtBl.MdtBlResource.AccidentReasonComment </label>

                        </td>
                        <td colspan="5">
                            <span class="multi">
                                @if (Model.BasicIncidentDetails.AccidentReason == "أخرى")
                                {
                                    @(Model.BasicIncidentDetails.AccidentReasonComment)
                                }
                                else
                                {
                                    @(Model.BasicIncidentDetails.AccidentReason ?? "")
                                }
                            </span>
                        </td>
                    </tr>

                </table>
                <br />

                @if (Model.VehicleDriverDetails != null)
                {
                    <table width="100%">
                        <tbody>

                            <tr>
                                <td colspan="6" style="border-bottom:1px solid #000000;">
                                    <h1 style="background-color:#cccccc; color:#000000;height:30px; width:150px;margin:0; line-height:30px; padding:15px; border-size:border-box;"> أسباب الحادث</h1>
                                </td>
                            </tr>
                            <tr>
                                <td>المركبة المتسببة</td>
                                <td>السبب</td>
                                <td>أخرى</td>
                                <td>رئيسي</td>
                            </tr>
                            @if (Model.VehicleDriverDetails != null)
                            {
                                foreach (var vehicle in Model.VehicleDriverDetails)
                                {
                                    if (vehicle.VehicleData.ReasonOfAccident != null)
                                    {
                                        <tr>
                                            <td>@(vehicle.VehicleData.PlateNumber ?? "")</td>
                                            <td>@(vehicle.VehicleData.ReasonOfAccident ?? "")</td>
                                            <td>@(!string.IsNullOrEmpty(vehicle.VehicleData.ReasonCategoryDescription) ? vehicle.VehicleData.ReasonCategoryDescription : "")</td>
                                            <td>
                                                رئيسي
                                            </td>
                                        </tr>
                                    }
                                }
                            }
                        </tbody>
                    </table>
                    <br />
                  
                        var seq = 1;
                    
                    // Arabic “التسلسل” starts at 1
               
                foreach (var vehicle in Model.VehicleDriverDetails)
                    {

                        <table width="100%" border="0">
                            <tr>
                                <td colspan="2" style="border-bottom:1px solid #000000;">
                                    <h3 style="background-color:#cccccc; color:#000000;height:30px; width:100%;margin:0; line-height:30px; padding:15px; border-size:border-box;">التسلسل &nbsp;  @seq</h3>
                                </td>
                                <td colspan="4" style="border-bottom:1px solid #000000;">
                                    <h1 style="background-color:#cccccc; color:#000000;height:30px; width:100%;margin:0; line-height:30px; padding:15px; border-size:border-box;">بيانات المركبة &nbsp; - &nbsp; @(vehicle.VehicleData.VehicleRole ?? "")</h1>

                                </td>
                            </tr>

                            <tr>
                                <td width="100px">
                                    <label>@MdtBl.MdtBlResource.PlateNumber</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.PlateNumber ?? "")</span>
                                </td>
                                <td width="100px">
                                    <label>@MdtBl.MdtBlResource.PlateColor</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.PlateColor ?? "")</span>
                                </td>

                                <td>
                                    <label>@MdtBl.MdtBlResource.PlateCategory</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.PlateCategory ?? "")</span>
                                </td>

                            </tr>
                            <tr class="alt">
                                <td>
                                    <label>@MdtBl.MdtBlResource.PlateType</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.PlateType ?? "")</span>
                                </td>
                                <td width="100px">
                                    <label>@MdtBl.MdtBlResource.PlateSource</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.PlateSource ?? "")</span>
                                </td>

                                <td>
                                    <label>لون المركبة</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.VehicleColor ?? "")</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <label>@MdtBl.MdtBlResource.VehicleBrand</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.VehicleBrand ?? "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.VehicleModel</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.VehicleModel ?? "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.VehicleType</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.VehicleType ?? "")</span>
                                </td>


                            </tr>
                            <tr class="alt">
                                <td>
                                    <label>@MdtBl.MdtBlResource.CountryOfOrigin</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.CountryOfOrigin ?? "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.ManufactureYear</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.ManufactureYear ?? "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.EngineNumber</label>
                                </td>
                                <td colspan="5">
                                    <span>@(vehicle.VehicleData.EngineNumber ?? "")</span>
                                </td>

                            </tr>
                            <tr>
                                <td>
                                    <label>@MdtBl.MdtBlResource.ChasisNumber</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.ChasisNumber ?? "")</span>
                                </td>

                            </tr>
                            <tr>
                                <td width="260px">
                                    <label>@MdtBl.MdtBlResource.VehicleDescription</label>
                                </td>
                                <td colspan="5">
                                    <span class="multi">@(vehicle.VehicleData.VehicleDesc ?? "")</span>
                                </td>
                            </tr>

                            <tr>
                                <td>
                                    <label> @MdtBl.MdtBlResource.InsuranceCompany </label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.InsuranceCompany ?? "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.InsuranceType</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.InsuranceType ?? "")</span>
                                </td>
                                <td>
                                    <label> @MdtBl.MdtBlResource.InsurancePolicyNumber </label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.InsurancePolicyNumber ?? "")</span>
                                </td>
                            </tr>
                            <tr class="alt">
                                <td>
                                    <label>@MdtBl.MdtBlResource.ExpiryDate</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.ExpiryDate.HasValue ? vehicle.VehicleData.ExpiryDate.Value.ToString("dd/MM/yyyy") : "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.CollisionPoint</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.CollisionPoint ?? "")</span>
                                </td>
                                <td>
                                    <label>@MdtBl.MdtBlResource.VehicleRole</label>
                                </td>
                                <td>
                                    <span>@(vehicle.VehicleData.VehicleRole ?? "")</span>
                                </td>


                            </tr>

                        </table>
                        <table width="100%">

                            <tr>
                                <td colspan="6" style="border-bottom:1px solid #000000;">
                                    <h1 style="background-color:#cccccc; color:#000000;height:30px; width:150px;margin:0; line-height:30px; padding:15px; border-size:border-box;"> المالك</h1>
                                </td>
                            </tr>
                            <tr>
                                <td style="width:100px;">
                                    <label>@MdtBl.MdtBlResource.Name</label>
                                </td>
                                <td colspan="5">
                                    <span>@(vehicle.OwnerData.Name ?? "")</span>
                                </td>

                            </tr>

                        </table>

                        <table width="100%">
                            <tbody>
                                <tr>
                                    <td>

                                        <label>@MdtBl.MdtBlResource.DamagedParts</label>
                                    </td>
                                </tr>
                                <tr class="alt">
                                    <td class="options" width="250px">
                                        @if (vehicle.DamagedPartData != null && vehicle.DamagedPartData.DamagedVehicleDetails != null)
                                        {
                                            foreach (var item in TemplateHelper.XmltoList(vehicle.DamagedPartData.DamagedVehicleDetails))
                                            {
                                                <div><span>&nbsp; &nbsp;</span>@item</div>
                                            }
                                        }
                                        @if (vehicle.DamagedPartData != null && vehicle.DamagedPartData.DamagePartsDescription != null && vehicle.DamagedPartData.DamagePartsDescription != "")
                                        {
                                            <div>
                                                <span>&nbsp; &nbsp;</span> @MdtBl.MdtBlResource.DamagePropertyTxt :    @vehicle.DamagedPartData.DamagePartsDescription
                                            </div>
                                        }
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <br />
                        <hr />
                        <br />
                        seq++;
                    }
                }

                <table dir="ltr" width="100%">
                    <tbody>
                        <tr>
                            <td colspan="6">
                                <hr style="background-color:#000000;color:#000000;" />
                            </td>

                        </tr>
                        <tr>

                            <td>website &nbsp; </td>
                            <td> www.saaed.ae </td>
                            <td>Fax &nbsp;</td>
                            <td> 02 556 3333</td>
                            <td>Tel &nbsp;</td>
                            <td>
                                <span>800</span>
                                <span>SAAED</span>
                                <span>(72233)</span>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="6">
                                <hr style="background-color:#000000;color:#000000;" />
                            </td>

                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>

</html> 